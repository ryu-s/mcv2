<UserControl x:Class="mcv2MainViewPlugin.Update.NotUpdate"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:mcv2MainViewPlugin.Update"
             mc:Ignorable="d" d:DesignHeight="410" d:DesignWidth="590">
    <Grid>
        <TextBlock Text="最新版を利用中なのでアップデートの必要はありません" FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Center" Width="440" />
        <Button x:Name="button" Content="閉じる" Command="{Binding CloseCommand}" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,5" Width="75"/>
    </Grid>
</UserControl>
